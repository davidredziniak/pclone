{
    "manifest_version": 2,
    "name": "Chrome Proxy",
	"description": "Auto Proxy",
	"version": "1.0",
    "permissions": [
        "proxy",
        "tabs",
        "<all_urls>",
        "webRequest",
		"webRequestAuthProvider",
        "webRequestBlocking"
    ],
    "background": {
        "scripts": ["background.js"],
		"persistent": true
    }
}